{
    "app-id" : "com.github.gravures.acide",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "_sdk-extensions" : [ "org.freedesktop.Sdk.Extension.llvm13" ],
    "command" : "acide",
    "finish-args" : [
        "--share=network",
